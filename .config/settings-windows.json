{
	"editor.fontFamily": "JetBrainsMono Nerd Font Mono",
	"editor.fontSize": 14,
	"editor.lineHeight": 1.5,
	"editor.fontWeight": "normal",
	"editor.fontLigatures": true,
	"editor.fontVariations": true,
	"editor.minimap.enabled": false,
	"editor.minimap.renderCharacters": false,
	"editor.accessibilitySupport": "off",
	"editor.aiStats.enabled": false,
	"editor.codeLens": true,
	"editor.codeLensFontFamily": "JetBrainsMono Nerd Font Mono",
	"editor.smoothScrolling": true,
	"editor.cursorSmoothCaretAnimation": "on",
	"editor.formatOnSave": true,
	"editor.defaultFormatter": "biomejs.biome",
	"editor.autoClosingBrackets": "always",
	"editor.autoClosingComments": "always",
	"editor.autoClosingQuotes": "always",
	"editor.autoClosingDelete": "always",
	"editor.autoClosingOvertype": "always",
	"editor.autoIndent": "full",
	"editor.bracketPairColorization.enabled": true,
	"editor.bracketPairColorization.independentColorPoolPerBracketType": true,
	"editor.colorDecorators": false,
	"editor.tabSize": 2,
	"editor.tabCompletion": "on",
	"editor.cursorBlinking": "expand",
	//
	"explorer.compactFolders": false,
	"explorer.decorations.badges": true,
	"explorer.decorations.colors": true,
	//
	"telemetry.telemetryLevel": "off",
	"telemetry.editStats.details.enabled": false,
	"telemetry.editStats.enabled": false,
	"telemetry.editStats.showDecorations": false,
	"telemetry.editStats.showStatusBar": false,
	"telemetry.feedback.enabled": false,
	"editor.experimental.treeSitterTelemetry": false,
	//
	"workbench.colorTheme": "cursor midnight theme",
	"workbench.productIconTheme": "fluent-icons",
	"workbench.iconTheme": "symbols",
	"workbench.layoutControl.enabled": false,
	"workbench.statusBar.visible": false,
	"workbench.startupEditor": "none",
	"workbench.activityBar.location": "top",
	"workbench.activityBar.iconClickBehavior": "focus",
	"workbench.commandPalette.history": 0,
	"workbench.reduceMotion": "on",
	"workbench.tips.enabled": false,
	"workbench.welcomePage.walkthroughs.openOnInstall": false,
	"workbench.sideBar.location": "left",
	//
	"window.commandCenter": false,
	"window.zoomLevel": -1,
	"window.newWindowProfile": "Default",
	"window.menuBarVisibility": "compact",
	"window.border": "off",
	"window.title": "${appName}",
	"window.restoreWindows": "none",
	//
	"update.showReleaseNotes": false,
	"update.mode": "manual",
	"update.enableWindowsBackgroundUpdates": false,
	//
	"chat.agent.enabled": false,
	"chat.commandCenter.enabled": false,
	"chat.extensionTools.enabled": false,
	//
	"extensions.allowed": {
		"*": true
	},
	"extensions.autoCheckUpdates": true,
	"extensions.autoUpdate": "onlyEnabledExtensions",
	"extensions.ignoreRecommendations": true,
	"extensions.verifySignature": false,
	//
	"security.workspace.trust.enabled": false,
	"security.workspace.trust.banner": "never",
	"security.workspace.trust.startupPrompt": "never",
	//
	"terminal.integrated.cursorBlinking": true,
	"terminal.integrated.defaultProfile.linux": "kitty",
	"terminal.integrated.defaultProfile.windows": "Git Bash",
	"terminal.integrated.confirmOnKill": "never",
	"terminal.integrated.smoothScrolling": true,
	"terminal.integrated.shellIntegration.decorationsEnabled": "both",
	"terminal.integrated.profiles.windows": {
		"Git Bash": {
			"source": "Git Bash",
			"color": "terminal.ansiWhite",
			"icon": "terminal-bash",
			"overrideName": true
		}
	},
	"terminal.integrated.confirmOnExit": "never",
	"terminal.integrated.suggest.enabled": false,
	"terminal.integrated.cursorStyle": "line",
	"terminal.integrated.fontFamily": "JetBrainsMono Nerd Font Mono",
	"terminal.integrated.fontSize": 14,
	//
	"errorLens.enabled": true,
	"errorLens.codeLensEnabled": false,
	"errorLens.fontFamily": "JetBrainsMono Nerd Font Mono",
	"errorLens.fontSize": "14",
	"errorLens.fontWeight": "500",
	"errorLens.enableOnDiffView": true,
	"errorLens.onSave": true,
	//
	"biome.enabled": true,
	//
	"excalidraw.theme": "dark",
	"excalidraw.language": "en",
	//
	"custom-ui-style.stylesheet": {
		".notification-toast": "box-shadow: none !important",
		".quick-input-widget.show-file-icons": "box-shadow: none !important",
		".quick-input-widget": "top: 25vh !important;",
		".quick-input-list .scrollbar": "display: none",
		".monaco-action-bar.quick-input-inline-action-bar": "display: none",
		".editor-widget.find-widget": "box-shadow: none; border-radius: 4px",
		".quick-input-titlebar": "background: #100B15 !important"
	},
	//
	"thunder-client.restoreTabs": false,
	"thunder-client.responseLimit": 1,
	"thunder-client.jsonFileIndentSize": 2,
	"thunder-client.indentSize": 2,
	"thunder-client.sidebar.layout": "compact",
	"thunder-client.sidebar.defaultTab": "Collections",
	"thunder-client.autoRunCurl": false,
	"thunder-client.httpLibrary": "axios",
	"thunder-client.requestTimeout": 15000,
	//
	"json.format.enable": true,
	"json.validate.enable": true,
	"json.schemaDownload.enable": true,
	//
	"vsintellicode.java.completionsEnabled": true,
	"vsintellicode.python.completionsEnabled": true,
	"vsintellicode.typescript.completionsEnabled": true,
	"vsintellicode.sql.completionsEnabled": true,
	//
	"l13Projects.showTagsInWorkspaces": false,
	"l13Projects.autoRemoveDeletedProjects": true,
	"l13Projects.sortWorkspacesBy": "name",
	"l13Projects.confirmDeleteFavorite": true,
	"l13Projects.confirmDeleteProject": true,
	"l13Projects.confirmDeleteTag": true,
	"l13Projects.confirmOpenMultipleWindows": true,
	//
}
