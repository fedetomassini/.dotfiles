{
	"editor.fontFamily": "JetBrainsMono Nerd Font Mono",
	"editor.fontSize": 14,
	"editor.lineHeight": 1.5,
	"editor.fontWeight": "normal",
	"editor.fontLigatures": true,
	"editor.fontVariations": true,
	"editor.minimap.enabled": true,
	"editor.minimap.renderCharacters": false,
	"editor.accessibilitySupport": "off",
	"editor.aiStats.enabled": false,
	"editor.codeLens": true,
	"editor.codeLensFontFamily": "JetBrainsMono Nerd Font Mono",
	"editor.smoothScrolling": true,
	"editor.cursorSmoothCaretAnimation": "on",
	"editor.formatOnSave": true,
	"editor.defaultFormatter": "biomejs.biome",
	"editor.autoClosingBrackets": "always",
	"editor.autoClosingComments": "always",
	"editor.autoClosingQuotes": "always",
	"editor.autoClosingDelete": "always",
	"editor.autoClosingOvertype": "always",
	"editor.autoIndent": "full",
	"editor.bracketPairColorization.enabled": true,
	"editor.bracketPairColorization.independentColorPoolPerBracketType": true,
	"editor.colorDecorators": false,
	"editor.tabSize": 2,
	"editor.tabCompletion": "on",
	"editor.cursorBlinking": "expand",
	//
	"explorer.compactFolders": false,
	"explorer.decorations.badges": true,
	"explorer.decorations.colors": true,
	//
	"telemetry.telemetryLevel": "off",
	"telemetry.editStats.details.enabled": false,
	"telemetry.editStats.enabled": false,
	"telemetry.editStats.showDecorations": false,
	"telemetry.editStats.showStatusBar": false,
	"telemetry.feedback.enabled": false,
	"editor.experimental.treeSitterTelemetry": false,
	"workbench.productIconTheme": "material-product-icons",
	"workbench.colorTheme": "Vesper",
	"workbench.iconTheme": "gruvbox-material-icon-theme",
	"workbench.layoutControl.enabled": false,
	"workbench.statusBar.visible": false,
	"workbench.startupEditor": "none",
	"workbench.activityBar.location": "top",
	"workbench.activityBar.iconClickBehavior": "focus",
	"workbench.commandPalette.history": 0,
	"workbench.reduceMotion": "on",
	"workbench.tips.enabled": false,
	"workbench.welcomePage.walkthroughs.openOnInstall": false,
	"workbench.sideBar.location": "left",
	"workbench.secondarySideBar.defaultVisibility": "hidden",
	//
	"window.commandCenter": false,
	"window.zoomLevel": 0,
	"window.newWindowProfile": "Default",
	"window.menuBarVisibility": "compact",
	"window.border": "off",
	"window.title": "${appName}",
	"window.restoreWindows": "none",
	//
	"update.showReleaseNotes": false,
	"update.mode": "manual",
	"update.enableWindowsBackgroundUpdates": false,
	//
	"chat.agent.enabled": false,
	"chat.commandCenter.enabled": false,
	"chat.extensionTools.enabled": false,
	//
	"extensions.allowed": {
		"*": true
	},
	"extensions.autoCheckUpdates": true,
	"extensions.autoUpdate": "onlyEnabledExtensions",
	"extensions.ignoreRecommendations": true,
	"extensions.verifySignature": false,
	//
	"security.workspace.trust.enabled": false,
	"security.workspace.trust.banner": "never",
	"security.workspace.trust.startupPrompt": "never",
	//
	"terminal.integrated.cursorBlinking": true,
	"terminal.integrated.defaultProfile.linux": "kitty",
	"terminal.integrated.defaultProfile.windows": "Git Bash",
	"terminal.integrated.confirmOnKill": "never",
	"terminal.integrated.smoothScrolling": true,
	"terminal.integrated.shellIntegration.decorationsEnabled": "both",
	"terminal.integrated.profiles.windows": {
		"Git Bash": {
			"source": "Git Bash",
			"color": "terminal.ansiWhite",
			"icon": "terminal-bash",
			"overrideName": true
		}
	},
	"terminal.integrated.confirmOnExit": "never",
	"terminal.integrated.suggest.enabled": false,
	"terminal.integrated.cursorStyle": "line",
	"terminal.integrated.fontFamily": "JetBrainsMono Nerd Font Mono",
	"terminal.integrated.fontSize": 14,
	//
	"errorLens.enabled": true,
	"errorLens.codeLensEnabled": false,
	"errorLens.fontFamily": "JetBrainsMono Nerd Font Mono",
	"errorLens.fontSize": "14",
	"errorLens.fontWeight": "500",
	"errorLens.enableOnDiffView": true,
	"errorLens.onSave": true,
	//
	"biome.enabled": true,
	//
	"excalidraw.theme": "dark",
	"excalidraw.language": "en",
	//
	"custom-ui-style.stylesheet": {
		".notification-toast": "box-shadow: none !important",
		".quick-input-widget.show-file-icons": "box-shadow: none !important",
		".quick-input-widget": "top: 25vh !important;",
		".quick-input-list .scrollbar": "display: none",
		".monaco-action-bar.quick-input-inline-action-bar": "display: none",
		".editor-widget.find-widget": "box-shadow: none; border-radius: 4px",
		".quick-input-titlebar": "background: #100B15 !important"
	},
	//
	"json.format.enable": true,
	"json.validate.enable": true,
	"json.schemaDownload.enable": true,
	//
	"scss.lint.unknownAtRules": "ignore",
	//
	"l13Projects.showTagsInWorkspaces": false,
	"l13Projects.autoRemoveDeletedProjects": true,
	"l13Projects.sortWorkspacesBy": "name",
	"l13Projects.confirmDeleteFavorite": true,
	"l13Projects.confirmDeleteProject": true,
	"l13Projects.confirmDeleteTag": true,
	"l13Projects.confirmOpenMultipleWindows": true,
	"biome.suggestInstallingGlobally": false,
	//
	"vscord.app.name": "Visual Studio Code",
	"vscord.app.id": "782685898163617802",
	"vscord.status.state.text.editing": "Editing {file_name}{file_extension} - {current_line}/{current_column} â˜•",
	"vscord.status.details.text.notInFile": "Idle ðŸ’¤",
	"vscord.status.state.text.idle": "Idle ðŸ’¤",
	"vscord.status.problems.text": "",
	"vscord.status.details.text.editing": "",
	//
	"glassit.alpha": 240,
	//
	"explorer.fileNesting.enabled": true,
	"explorer.fileNesting.patterns": {
		"*.ts": "${capture}.js",
		"*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
		"*.jsx": "${capture}.js",
		"*.tsx": "${capture}.ts",
		"*.env": "${capture}.env",
		"tsconfig.json": "tsconfig.*.json",
		"package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, bun.lock"
	},
	//
	"[php]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[javascript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[typescript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[json]": {
		"editor.defaultFormatter": "biomejs.biome"
	}
}
